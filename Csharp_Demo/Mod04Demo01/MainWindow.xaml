<Window x:Class="Mod04Demo01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mod04Demo01"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="constructors" Content="Constructors" HorizontalAlignment="Left" VerticalAlignment="Top" Width="154" Height="37" Click="constructors_Click"/>
        <Button x:Name="statics" Content="Statics" HorizontalAlignment="Left" Margin="159,0,0,283.8" VerticalAlignment="Bottom" Width="177" Height="37" Click="statics_Click"/>
        <ListBox x:Name="customerList" HorizontalAlignment="Left" Height="279" Margin="0,42,0,-0.2" VerticalAlignment="Top" Width="251" SelectionChanged="customerList_SelectionChanged"/>
        <TextBox x:Name="details" HorizontalAlignment="Left" Height="279" Margin="251,42,0,-0.2" TextWrapping="Wrap" VerticalAlignment="Top" Width="267"/>
        <Button x:Name="generic" Content="Generic" HorizontalAlignment="Left" VerticalAlignment="Top" Width="177" Margin="341,0,0,0" Height="37"/>

    </Grid>
</Window>
